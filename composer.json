{
    "name": "cbaconnier/laravel-http-pool-callback",
    "description": "A Laravel package that enhances the HTTP pool functionality by allowing users to apply callbacks to the results.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cl√©ment Baconnier",
            "email": "clement@baconnier.ch"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Cbaconnier\\HttpPool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cbaconnier\\HttpPool\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cbaconnier\\HttpPool\\HttpPoolServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^8.12",
        "phpunit/phpunit": "^10.3",
        "guzzlehttp/guzzle": "^7.8",
        "larastan/larastan": "^2.9",
        "phpstan/extension-installer": "^1.3"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
